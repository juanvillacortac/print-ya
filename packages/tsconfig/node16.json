{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "target": "ES2021",
    "strict": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true
  }
}
