    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "build:node": "ADAPTER=node svelte-kit build",
    "start:node": "node build/index.js",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "package": "svelte-kit package",
    "gql:fetch": "gq https://graphql.datocms.com/ -H \"Authorization: Bearer $DATOCMS_API_KEY\" --introspect > src/lib/schema.graphql",
    "postbuild@comment": "cp node_modules/sharp/build .vercel_build_output/functions/node/render -r;cp node_modules/sharp/vendor/8.11.3/linux-x64/lib/libvips-cpp.so.42 .vercel_build_output/functions/node/render/;ls .vercel_build_output/functions/node/render/",
    "vercel-build@comment": "ls node_modules/@prisma && prisma generate && cross-env VITE_DATABASE_URL=$MIGRATION_DB prisma migrate deploy && svelte-kit build && cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/ && cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build@Caca": "prisma generate && svelte-kit build && cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/ && cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build@2": "prisma generate && svelte-kit build && cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/ && cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build": "prisma generate && svelte-kit build"
  },
